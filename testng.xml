<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sample Suite" parallel="false">
    <listeners>
        <listener class-name="listerners.TestListener" />
    </listeners>
    <test name="Login Test">
        <classes>
            <class name="tests.LoginTest"/>
        </classes>
    </test>
    <test name="Restaurant Test">
        <classes>
            <class name="tests.restaurantTest"/>
        </classes>
    </test>
    <test name="User Test">
        <classes>
            <class name="tests.UserTest"/>
        </classes>
    </test>
    <test name="System Variable Test">
        <classes>
            <class name="tests.SystemVariableTest"/>
        </classes>
    </test>
    <test name="Account Test">
        <classes>
            <class name="tests.accountTest"/>
        </classes>
    </test>
    <test name="Logout Test">
        <classes>
            <class name="tests.logoutTest"/>
        </classes>
    </test>
</suite>